<div class = "row">
	<aside class = "span3/2 offset3">
			<div id="runsqajob_form">
				<% if running?(@sqajob) == "notrun" || running?(@sqajob) == "waiting" %>
						<%= render 'sqajobrun' %>
				<% elsif running?(@sqajob) == "running" || running?(@sqajob) == "stopped"%>
						<%= render 'sqajobrunning' %>
				<% elsif running?(@sqajob) == "rundone" %>
						<%= render 'sqajobrun' %>
				<% end %>
			</div>
	</aside>
	<aside class = "span3/2">
			<div id="stopsqajob_form">
				<% if running?(@sqajob) == "notrun" || running?(@sqajob) == "stopped" || running?(@sqajob) == "waiting" %>
						<%= render 'sqajobdisablestop' %>
				<% elsif running?(@sqajob) == "running" %>
						<%= render 'sqajobstop' %>
				<% elsif running?(@sqajob) == "rundone" %>
						<%= render 'sqajobdisablestop' %>
				<% end %>
			</div>
	</aside>
	<aside class = "span3/2">
			<div id="logsqajob_form">
				<% if running?(@sqajob) == "notrun" || running?(@sqajob) == "stopped" || running?(@sqajob) == "waiting" %>
						<%= render 'sqajobdisablelog' %>
				<% else %>
						<%= render 'sqajoblog' %>
				<% end %>
			</div>
	</aside>

	<aside class = "span3/2">
			<div id="resultsqajob_form">
				<% if running?(@sqajob) == "rundone" %>
						<%= render 'sqajobresult' %>
				<% else %>
						<%= render 'sqajobdisableresult' %>
				<% end %>
			</div>
	</aside>

	<aside class = "span3/2">
			<%= link_to "Delete", @sqajob, method: :delete, data: { confirm: "You Sure?" }, class: "btn btn-large btn-warning" %>
	</aside>

	<aside class = "span1">
			<%= link_to "Back", sqajobs_path, class: "btn btn-large btn-primary" %>
	</aside>
</div>
<div class = "row">
	<aside class = "span10 offset2">
		<div id = "run_sqajob_detail_log" >
		</div>
	</aside>
</div>