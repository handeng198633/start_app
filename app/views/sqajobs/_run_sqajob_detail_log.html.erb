<% require 'coderay' %>
<% File.open("#{Rails.root}/public/output.txt", "r") do |file| %>
	<% while line = file.gets %>
		<% if not line =~ /^$/ %>
			<% line = log2html_before(line) %>
			<% line = CodeRay.scan("#{line}", :ruby).div %>
			<% line = log2html_after(line) %>						
			<%= line.html_safe %>
		<% end %>
	<% end %>
<% end %>
