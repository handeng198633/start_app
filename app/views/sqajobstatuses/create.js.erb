<% if Sqajob.where(job_state: 'running').take.nil? %>
	$("#runsqajob_form").html("<%= escape_javascript(render('sqajobs/sqajobrunning')) %>")
	$("#stopsqajob_form").html("<%= escape_javascript(render('sqajobs/sqajobstop')) %>")
	$("#logsqajob_form").html("<%= escape_javascript(render('sqajobs/sqajoblog')) %>")
<% else %>
	$("#runsqajob_form").html("<%= escape_javascript(render('sqajobs/sqajobwaiting')) %>")
<% end %>
